{
  "name": "@twa.js/sdk-react",
  "version": "0.1.8",
  "description": "React bindings for Web Apps client SDK. Contains hooks, components and other useful tools which allow usage of React along with Web Apps client SDK.",
  "author": "Vladislav Kibenko <wolfram.deus@gmail.com>",
  "homepage": "https://github.com/Telegram-Web-Apps/twa#readme",
  "keywords": [
    "telegram",
    "telegram-web-apps",
    "twa",
    "sdk",
    "react"
  ],
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.cjs",
  "browser": "dist/browser.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "web": "./dist/index.mjs",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Telegram-Web-Apps/twa.git",
    "directory": "packages/sdk"
  },
  "scripts": {
    "dev": "rollup --watch --config ./playground/rollup.config.js",
    "test": "echo \"Tests not specified\"",
    "build": "rollup --config rollup.config.js"
  },
  "bugs": {
    "url": "https://github.com/Telegram-Web-Apps/twa/issues"
  },
  "peerDependencies": {
    "@types/react": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "dependencies": {
    "@twa.js/bridge": "workspace:*",
    "@twa.js/sdk": "workspace:*"
  }
}
