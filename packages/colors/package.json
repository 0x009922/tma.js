    "./*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/lib/*.mjs",
      "require": "./dist/lib/*.cjs",
      "default": "./dist/lib/*.cjs"
    }
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint -c .eslintrc.cjs src/**/* __tests__/**/*",
    "build": "vite build"
  },
  "devDependencies": {
    "tsconfig": "workspace:*",
    "jest-config-custom": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "build-utils": "workspace:*"
  },
  "publishConfig": {
