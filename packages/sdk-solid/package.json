{
  "name": "@tma.js/sdk-solid",
  "version": "0.1.3",
  "description": "Solid JS bindings for Web Apps client SDK.",
  "author": "Vladislav Kibenko <wolfram.deus@gmail.com>",
  "homepage": "https://github.com/Telegram-Mini-Apps/tma.js#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:Telegram-Mini-Apps/tma.js.git",
    "directory": "packages/sdk-solid"
  },
  "bugs": {
    "url": "https://github.com/Telegram-Mini-Apps/tma.js/issues"
  },
  "keywords": [
    "telegram-mini-apps",
    "typescript",
    "sdk",
    "solidjs"
  ],
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "solid": "./dist/source/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./*": {
      "types": "./dist/types/*.d.ts",
      "solid": "./dist/source/*.js",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js",
      "node": "./dist/cjs/*.js",
      "default": "./dist/cjs/*.js"
    }
  },
  "scripts": {
    "lint": "eslint -c .eslintrc.cjs src/**/*",
    "build": "vite build"
  },
  "dependencies": {
    "@tma.js/utils": "workspace:*",
    "@tma.js/sdk": "workspace:*"
  },
  "peerDependencies": {
    "solid-js": "^1.0.0"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "build-utils": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
