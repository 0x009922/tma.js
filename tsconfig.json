{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "strictBindCallApply": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "declaration": true,
    "sourceMap": true,
    "inlineSources": true,
    "target": "ES5",
    "module": "ES6",
    "moduleResolution": "node",
    "lib": [
      "ESNext",
      "dom"
    ]
  }
}